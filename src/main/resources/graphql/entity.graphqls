type Account {
    email: String!,
    password: String!,
    nickname: String,
    isLive: Boolean
}

type Post {
    id: Int!,
    title: String!,
    content: String!,
    emotion: Emotion,
    date: String,
    account: Account,
    isLive: Boolean
}

type Emotion {
    emotion: String
}

type WriteDate {
    id: Int,
    years: [Int],
    months: [Int],
    dates: [String],
    all: [DayPostInfo]
}

type DayPostInfo {
    day: String,
    postId: Int,
    title: String
}